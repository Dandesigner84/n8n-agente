{
  "manifest_version": 3,
  "name": "N8N Architect AI",
  "version": "1.0",
  "description": "Seu assistente de IA para criar e gerenciar workflows do n8n.",
  "icons": {
    "16": "icon.png", 
    "48": "icon.png",
    "128": "icon.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}